{
  "name": "polyseek",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=20"
  },
  "type": "module",
  "scripts": {
    "dev": "next dev -p 3000",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "db:test:up": "docker compose --env-file .env.test up -d test-db",
    "db:test:migrate": "dotenv -e .env.test -- npx prisma migrate deploy",
    "test:e2e:run": "npx playwright test",
    "test:e2e": "npm run db:test:up && npm run db:test:migrate && npx prisma db seed && npm run test:e2e:run"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.9.1",
    "@fortawesome/fontawesome-svg-core": "^7.0.1",
    "@fortawesome/free-solid-svg-icons": "^7.0.1",
    "@fortawesome/react-fontawesome": "^3.0.2",
    "@heroicons/react": "^2.2.0",
    "@radix-ui/react-checkbox": "^1.3.1",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.14",
    "@radix-ui/react-label": "^2.1.6",
    "@radix-ui/react-scroll-area": "^1.2.8",
    "@radix-ui/react-select": "^2.2.4",
    "@radix-ui/react-slider": "^1.3.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tailwindcss/typography": "^0.5.16",
    "autoprefixer": "^10.4.21",
    "cheerio": "^1.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dompurify": "^3.2.7",
    "embla-carousel-react": "^8.6.0",
    "lucide-react": "^0.510.0",
    "next": "15.3.1",
    "next-auth": "^5.0.0-beta.27",
    "next-themes": "^0.4.6",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-markdown": "^10.1.0",
    "tailwind-merge": "^3.3.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.56.1",
    "@prisma/client": "^6.19.0",
    "@tailwindcss/postcss": "^4",
    "@testing-library/react": "^16.3.0",
    "@types/dompurify": "^3.2.0",
    "@types/jsdom": "^27.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^5.1.0",
    "@vitest/ui": "^4.0.7",
    "cross-env": "^10.1.0",
    "dotenv": "^17.2.3",
    "dotenv-cli": "^11.0.0",
    "eslint": "^9",
    "eslint-config-next": "15.3.1",
    "jsdom": "^27.1.0",
    "tailwindcss": "^4",
    "ts-node": "^10.9.1",
    "tsx": "^4.19.4",
    "tw-animate-css": "^1.2.9",
    "typescript": "^5",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^4.0.7"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  }
}
